  <canvas id="myChart" width="800" height="400"></canvas>


<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
<script>
  
  const ctx = document.getElementById('myChart').getContext('2d');
  let data = JSON.parse( document.getElementById('orderByDay').value)
console.log(data);


  // Initialize chart with default data
  const myChart = new Chart(ctx, {
    type: 'bar',
    data: {
      // labels: daysOfWeek,
      datasets: [{
        label: '# of Orders Placed',
        data: data, // Initially, empty data
        backgroundColor: 'rgba(54, 162, 235, 1)',
        borderColor: 'rgba(54, 162, 235, 1)',
        borderWidth: 2,
        barThickness: 50
      }]
    },
    options: {
      scales: {
        y: {
          beginAtZero: true
        }
      },
      plugins: {
        legend: {
          display: true,
          labels: {
            color: 'rgb(0, 0, 0)',
            font: {
              size: 14
            }
          }
        },
        title: {
          display: true,
          text: 'Orders Placed by Day',
          color: 'rgb(0, 0, 0)',
          font: {
            size: 18,
            weight: 'bold'
          }
        }
      }
    }
  });


   
  async function getReport(reportType) {
    try {
      // Fetch data from the server
      const response = await fetch(`admin/adminChart?report_type=${reportType}`, {
               method: 'GET',
                 headers: {
                     'Content-Type': 'application/json', // Adjust the content type according to your needs
                }
});
const {data} = await response.json();
 

      // Update chart data based on the fetched data
      updateChartData(data);
      return data
    } catch (error) {
      console.error('Error fetching report:', error);
    }
  }

  // Function to update chart data
  function updateChartData(newData) {
   
    myChart.data.datasets[0].data = newData;
    myChart.update();
  }

  // On page load or wherever appropriate, call getReport with appropriate report type
 
</script>

              
                 
