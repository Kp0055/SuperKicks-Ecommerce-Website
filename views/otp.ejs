<body>

    <section class="container-fluid bg-body-tertiary d-block">
        <div class="row justify-content-center">
            <div class="col-12 col-md-6 col-lg-4" style="min-width: 500px;">
              <div class="card bg-white mb-5 mt-5 border-2" style="box-shadow: 0 12px 15px rgba(0, 0, 0, 0.02);">
                <div class="card-body p-5 text-center">
                  <h4>Verify</h4>
                  <p>Your code was sent to you via email</p>
      
                  <div  class="otp-field mb-4 ">
                    <input type="number" id="otp1" )"/>
                    <input type="number" id="otp2" )" />
                    <input type="number" id="otp3" )"/>
                    <input type="number" id="otp4" )"/>
                    <input type="number" id="otp5" )"/>
                    <input type="number" id="otp6" )"/>
                  </div>
      
                  <button class="btn btn-primary mb-3"  onclick="verifyOtp()">
                    Verify
                  </button>
      
                  <!-- <p class="resend text-muted mb-0">
                    Didn't receive code? <a href="resendOtp">Request again</a>
                  </p> -->
                </div>
              </div>
            </div>
          </div>
      </section>
